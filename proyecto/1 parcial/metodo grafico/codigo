"import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "def y(x):\n",
    "    return (2*x**2-5*x + 3)\n",
    "\n",
    "xarray = np.linspace(0,2,100)\n",
    "largo = len(xarray)\n",
    "yarray = np.zeros(largo)\n",
    "\n",
    "for i in range (largo):\n",
    "    yarray[i] = y(xarray[i])\n",
    "    \n",
    "plt.plot(xarray,yarray)\n",
    "plt.grid()"
